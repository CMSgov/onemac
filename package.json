{
  "name": "macpro-quickstart-serverless",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "nightwatch"
  },
  "private": "true",
  "scripts": {
    "test": "node nightwatch -c ./nightwatch/conf/nightwatch.conf.js --reporter ./nightwatch/html-reporter.js",
    "login-test": "npm run chrome ./nightwatch/tests/cases/OY2-1494_Test_SPA_Login_Dev.js",
    "chrome": "npm test -- -e chrome",
    "cross-browser": "npm test -- -e chrome,firefox",
    "firefox": "npm test -- -e firefox",
    "unit-test-front": "cd services/ui-src/ && npm run test",
    "unit-test-back": "cd services/ui-src/ && npm run test",
    "unit-test-nowatch": "cd services/ui-src && npm run test-nowatch",
    "coverage": "cd services/ui-src && npm run coverage",
    "regression": "npm run chrome -- --tag regression --skiptags unit,integration,devEnv",
    "regression-dev": "npm run chrome -- --tag devEnv --skiptags unit,integration",
    "smoke": "npm run chrome -- --tag smoke --skiptags unit,integration,regression",
    "update": "npx npm-check-updates -u && npm install",
    "integration": "npm test -- -e unit-test --tag integration --skiptags unit"
  },
  "unsafe-perm": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CMSgov/macpro-quickstart-serverless.git"
  },
  "author": "",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/CMSgov/macpro-quickstart-serverless/issues"
  },
  "homepage": "https://github.com/CMSgov/macpro-quickstart-serverless#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "chromedriver": ">=86.x",
    "dotenv": "^8.2.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "geckodriver": "^1.21.1",
    "handlebars": "^4.7.7",
    "lodash": "^4.17.20",
    "nightwatch": "^1.5.1",
    "selenium-server": "^3.141.59",
    "serverless": "^2.18.0",
    "sinon": "^9.2.3"
  }
}
