{
  "name": "macpro-quickstart-serverless",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "nightwatch"
  },
  "private": "true",
  "scripts": {
    "test": "node nightwatch.js -c nightwatch/conf/nightwatch.conf.js",
    "test:cypress": "cd tests/cypress && npm test && cd -",
    "test:cypress:ci": "cd tests/cypress && npm run test:ci && cd -",
    "chrome": "npm test -- -e chrome",
    "chromeHeadless": "npm test -- -e chromeHeadless",
    "cross-browser": "npm test -- -e chrome,firefox",
    "firefox": "npm test -- -e firefox",
    "unit": "npm test -- -e unit-test --skiptags=smoke,regression,integration",
    "login": "npm run chrome -- nightwatch/tests/cases/OY2-1494_Test_SPA_Login.js",
    "regression": "npm run chrome -- --tag regression --skiptags unit,integration,devEnv",
    "regression-dev": "npm run chromeHeadless -- nightwatch/tests/cases nightwatch/tests/suites",
    "regression-soon": "npm run chrome -- --tag regression-soon --skiptags unit,integration,devEnv,regression",
    "regression-master": "npm run chromeHeadless -- --tag regression-soon --skiptags unit,integration,devEnv,regression",
    "smoke-test": "npm run chrome -- --tag smokeTest --skiptags unit,integration,regression",
    "update": "npx npm-check-updates -u && npm install",
    "integration": "npm test -- -e unit-test --tag integration --skiptags unit",
    "prepare": "husky install"
  },
  "unsafe-perm": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CMSgov/macpro-quickstart-serverless.git"
  },
  "author": "",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/CMSgov/macpro-quickstart-serverless/issues"
  },
  "homepage": "https://github.com/CMSgov/macpro-quickstart-serverless#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "chai": "^4.2.0",
    "chromedriver": ">=90.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "geckodriver": "^1.21.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "lodash": "^4.17.20",
    "nightwatch": "^1.5.1",
    "prettier": "^2.3.1",
    "selenium-server": "^3.141.59",
    "serverless": "2.19.0",
    "sinon": "^9.2.3",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,scss,css,md}": [
      "prettier --write"
    ]
  }
}
